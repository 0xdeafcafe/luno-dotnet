{
	"version": "0.1.0-alpha-*",
	"title": "Luno Client",
	"description": "A dotnet wrapper for the https://luno.io/ api",
	"summary": "A dotnet wrapper for the https://luno.io/ api, with support for dotnet core.",
	"authors": [
		"Alex Forbes-Reed"
	],
	"tags": [
		"luno",
		"luno",
		"luno io",
		"bugatti",
		"spanko"
	],
	"language": "en-gb",
	"copyright": "Alex Forbes-Reed 2016",
	"projectUrl": "https://github.com/0xdeafcafe/luno-dotnet",
	"licenseUrl": "https://github.com/0xdeafcafe/luno-dotnet/blob/master/LICENSE",
	"repository": {
		"type": "git",
		"url": "https://github.com/0xdeafcafe/luno-dotnet"
	},
	"tooling": {
		"defaultNamespace": "LunoClient"
	},
	"dependencies": {
		"Newtonsoft.Json": "7.0.1"
	},
	"configurations": {
		"Debug": {
			"compilationOptions": {
				"define": [ "DEBUG", "TRACE" ]
			}
		},
		"Release": {
			"compilationOptions": {
				"define": [ "RELEASE", "TRACE" ],
				"optimize": true
			}
		}
	},
	"compilationOptions": {
		"languageVersion": "csharp6",
		"warningsAsErrors": true
	},
	"frameworks": {
		"net451": {
			"dependencies": {
				"System.Net.Http": "4.0.1-beta-23516"
			}
		},
		"dotnet5.4": {
			"dependencies": {
				"Microsoft.CSharp": "4.0.1-beta-23516",
				"System.Collections": "4.0.11-beta-23516",
				"System.Linq": "4.0.1-beta-23516",
				"System.Runtime": "4.0.21-beta-23516",
				"System.Threading": "4.0.11-beta-23516",
				"System.Net.Http": "4.0.1-beta-23516",
				"System.Text.Encoding": "4.0.11-beta-23516",
				"System.Text.Encoding.Extensions": "4.0.11-beta-23516",
				"System.Security.Cryptography.Algorithms": "4.0.0-beta-23516"
			}
		}
	}
}
